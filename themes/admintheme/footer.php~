			$('#campaign_id').on('change',function(){
				//var type = $(this).prop('id');
				//$('#leadYearToDate').css('display','block');
				//$('#leadMonthToDate').css('display','block');
				var type = $(this).find('option:selected').attr('id');
				var company_id = $(this).val();
				//alert(type);
				//alert(company_id);
				if(company_id!='' && type!='')
				window.location.href = "<?php echo base_url().'admin/dashboard/'?>"+company_id+'/'+type;
			});
		});
		</script>
		
